{
  "extends": ["callstack-io"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "no-underscore-dangle": 0,
    "no-console": 0,
    "flowtype/no-weak-types": 0,
    "import/prefer-default-export": 0,
    "react/forbid-prop-types": [0, { "forbid": ["object"] }]
  },
  "globals": {
    "cy": true,
    "context": true
  },
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  }
}
